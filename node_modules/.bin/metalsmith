#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ricky/Development/template-test/node_modules/.pnpm/metalsmith@2.6.3/node_modules/metalsmith/bin/node_modules:/Users/ricky/Development/template-test/node_modules/.pnpm/metalsmith@2.6.3/node_modules/metalsmith/node_modules:/Users/ricky/Development/template-test/node_modules/.pnpm/metalsmith@2.6.3/node_modules:/Users/ricky/Development/template-test/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ricky/Development/template-test/node_modules/.pnpm/metalsmith@2.6.3/node_modules/metalsmith/bin/node_modules:/Users/ricky/Development/template-test/node_modules/.pnpm/metalsmith@2.6.3/node_modules/metalsmith/node_modules:/Users/ricky/Development/template-test/node_modules/.pnpm/metalsmith@2.6.3/node_modules:/Users/ricky/Development/template-test/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../metalsmith/bin/metalsmith" "$@"
else
  exec node  "$basedir/../metalsmith/bin/metalsmith" "$@"
fi
